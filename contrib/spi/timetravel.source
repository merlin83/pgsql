DROP FUNCTION timetravel();
DROP FUNCTION set_timetravel(name, int4);

CREATE FUNCTION timetravel() 
	RETURNS opaque 
	AS '_OBJWD_/timetravel_DLSUFFIX_'
	LANGUAGE 'newC';

CREATE FUNCTION set_timetravel(name, int4) 
	RETURNS int4 
	AS '_OBJWD_/timetravel_DLSUFFIX_'
	LANGUAGE 'newC' WITH (isStrict);
