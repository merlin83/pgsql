QUERY: CREATE TABLE TEMP_DATETIME (f1 datetime);
QUERY: INSERT INTO TEMP_DATETIME (f1)
  SELECT d1 FROM DATETIME_TBL
  WHERE d1 BETWEEN '13-jun-1957' AND '1-jan-1997'
   OR d1 BETWEEN '1-jan-1999' AND '1-jan-2010';
