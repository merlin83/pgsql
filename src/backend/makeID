#!/bin/sh
#
# $Header: /home/rubik/work/pgcvs/CVSROOT/pgsql/src/backend/Attic/makeID,v 1.1 1996-07-09 06:21:08 scrappy Exp $
#


if test $# -ne 1; then
    echo "usage: $0 PORTNAME"
    exit 1
fi
PORTNAME=$1
 
gfind . port/$PORTNAME -type f  -name '*.[chly]' -print | mkid -S.gen=C - 2>&1 | grep -v 'No scanner for language'



exit 0
