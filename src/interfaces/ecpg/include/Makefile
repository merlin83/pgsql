	rm -f $(DESTDIR)$(HEADERDIR)/ecpgerrno.h
	rm -f $(DESTDIR)$(HEADERDIR)/ecpglib.h
	rm -f $(DESTDIR)$(HEADERDIR)/ecpgtype.h
	rm -f $(DESTDIR)$(HEADERDIR)/sqlca.h

