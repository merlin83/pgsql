#: fe-auth.c:232
#, c-format
msgid "Kerberos 4 error: %s\n"
msgstr "ошибка Kerberos 4: %s\n"

#: fe-auth.c:394
#, c-format
msgid "could not set socket to blocking mode: %s\n"
msgstr "не получилось установить сокет в блокирующий режим: %s\n"

