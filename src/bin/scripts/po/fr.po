#, c-format
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Options :\n"

#: createdb.c:223
#, c-format
msgid "  -D, --location=PATH       alternative place to store the database\n"
msgstr ""
"  -D, --location=CHEMIN       autre emplacement de stockage de la base de\n"
"                              données\n"

