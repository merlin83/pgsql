msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Opzioni:\n"

#: createdb.c:217
msgid "  -D, --location=PATH       alternative place to store the database\n"
msgstr "  -D, --location=PERCORSO   percorso alternativo in cui salvare il database\n"

