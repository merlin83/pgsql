msgid "%s: unrecognized shutdown mode \"%s\"\n"
msgstr "%s: geçersiz kapanma modu \"%s\"\n"

#: pg_ctl.c:1169
#, c-format
msgid "%s: unrecognized signal name \"%s\"\n"
msgstr "%s: geçersiz sinyal adı \"%s\"\n"

