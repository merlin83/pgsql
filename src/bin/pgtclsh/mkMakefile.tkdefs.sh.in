
if [ ! -f @TK_CONFIG_SH@ ]; then
    echo "@TK_CONFIG_SH@ not found"
    echo "I need this file! Please make a symbolic link to this file"
    echo "and start make again."
    exit 1
fi

. @TK_CONFIG_SH@

cat @TK_CONFIG_SH@ |
    egrep '^TK_' |
    while read inp
    do
	    eval eval echo $inp
    done >Makefile.tkdefs

exit 0
