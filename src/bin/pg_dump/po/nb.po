msgid ""
"  -O                       do not output \\connect commands in plain\n"
"                           text format\n"
msgstr "  -O                       ikke skriv \\connect-kommandoen i rent tekstformat\n"

#: pg_dump.c:711
msgid ""
"  -R                       disable ALL reconnections to the database in\n"
