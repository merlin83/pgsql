msgid "options \"clean\" (-c) and \"data only\" (-a) cannot be used together\n"
msgstr "選項\"clean\" (-c) and \"data only\" (-a)不能一起使用\n"

#: pg_dump.c:447
msgid "large-object output not supported for a single table\n"
msgstr "不支援單一資料表備份large-object\n"

