#, c-format
msgid "%s: too many command-line arguments (first is \"%s\")\n"
msgstr "%s: 過多命令列參數(第一個是\"%s\")\n"

#: pg_dump.c:435
msgid "options \"schema only\" (-s) and \"data only\" (-a) cannot be used together\n"
msgstr "選項\"schema only\" (-s)和\"data only\" (-a)不能一起使用\n"

